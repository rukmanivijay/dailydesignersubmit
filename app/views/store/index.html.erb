<% if notice %>
<p id="notice"><%= notice %></p>
<% end %>
<h1>Summer Wardrobe</h1>
<% @products.each do |product| %>
<div class="entry">
	<h3><%= product.title %></h3>
	<%= image_tag(product.image_url) %>
</div>
<%= button_to 'Dress up', line_items_path(:product_id => product) %>
<br><br>
<% end %>